{
  "name": "gulp-elm-find-dependencies",
  "version": "0.1.0",
  "description": "A gulp plugin wrapping the find-elm-dependencies module that returns the deps as vinyl objects",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "test": "mocha ./test/**.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --no-semi --single-quote --trailing-comma all",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farism/gulp-elm-find-dependencies.git"
  },
  "keywords": [
    "gulpplugin",
    "elm"
  ],
  "author": "Faris Mustafa <farismmk@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/farism/gulp-elm-find-dependencies/issues"
  },
  "homepage": "https://github.com/farism/gulp-elm-find-dependencies#readme",
  "engines": {
    "node": ">=4.2.0"
  },
  "dependencies": {
    "find-elm-dependencies": "^1.0.1",
    "gutil": "^1.6.4",
    "lint-staged": "^4.0.3",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "husky": "^0.14.3",
    "mocha": "^3.5.0",
    "prettier": "^1.5.3"
  }
}
